<view class='nav-wrap' style='height: {{height*2 + 20}}px;'>

  <view class='nav-title' style='line-height: {{height*2 + 44}}px;'>{{navbarData.title}}</view>
  <view style='display: flex; justify-content: space-around;flex-direction: column'>

    <view class='nav-capsule' style='height: {{height*2 + 44}}px;' wx:if="{{ back || home }}">

      <view bindtap='_navback' wx:if="{{back}}">
        <image src='/image/back-pre.png' mode='aspectFill' class='back-pre'></image>
      </view>
      <view class='navbar-v-line'></view>
      <view bindtap='_backhome' wx:if="{{home}}">
        <image src='/image/back-home.jpg' mode='aspectFill' class='back-home'></image>
      </view>
    </view>
  </view>
</view>
